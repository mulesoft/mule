<plugin>

   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="org.mule.ide.ui.preferences.MulePreferencePage"
            id="org.mule.ide.ui.preferences.MulePreferencePage"
            name="Mule UMO">
