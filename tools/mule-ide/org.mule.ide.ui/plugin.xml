            objectClass="org.eclipse.core.resources.IProject">
         <keywordReference id="org.mule.ide.ui.keyword.mule"/>
      </page>
   </extension>
   <extension
         point="org.eclipse.ui.keywords">
      <keyword
            id="org.mule.ide.ui.keyword.mule"
            label="Mule"/>
   </extension>

</plugin>
