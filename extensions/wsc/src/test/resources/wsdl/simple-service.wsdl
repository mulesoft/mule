<part name="parameters" element="tns:echoAccount"/>
</message>
<message name="echoAccountResponse">
<part name="parameters" element="tns:echoAccountResponse"/>
</message>
<portType name="TestService">
<operation name="echo">
    <input wsam:Action="echoOperation" message="tns:echo"/>
    <output wsam:Action="http://consumer.ws.extension.mule.org/TestService/echoResponse" message="tns:echoResponse"/>
</operation>
<operation name="fail">
    <input wsam:Action="fail" message="tns:fail"/>
    <output wsam:Action="http://consumer.ws.extension.mule.org/TestService/failResponse" message="tns:failResponse"/>
    <fault message="tns:EchoException" name="EchoException" wsam:Action="http://consumer.ws.extension.mule.org/TestService/fail/Fault/EchoException"/>
</operation>
<operation name="echoWithHeaders" parameterOrder="parameters headerIn headerOut headerInOut">
    <input wsam:Action="echoWithHeaders" message="tns:echoWithHeaders"/>
    <output wsam:Action="http://consumer.ws.extension.mule.org/TestService/echoWithHeadersResponse" message="tns:echoWithHeadersResponse"/>
    <fault message="tns:EchoException" name="EchoException" wsam:Action="http://consumer.ws.extension.mule.org/TestService/echoWithHeaders/Fault/EchoException"/>
</operation>
<operation name="noParams">
    <input wsam:Action="noParams" message="tns:noParams"/>
    <output wsam:Action="http://consumer.ws.extension.mule.org/TestService/noParamsResponse" message="tns:noParamsResponse"/>
</operation>
<operation name="noParamsWithHeader" parameterOrder="parameters headerIn">
    <input wsam:Action="noParams" message="tns:noParamsWithHeader"/>
    <output wsam:Action="http://consumer.ws.extension.mule.org/TestService/noParamsWithHeaderResponse" message="tns:noParamsWithHeaderResponse"/>
</operation>
<operation name="echoAccount">
