            <version>${project.version}</version>
        </dependency>
        <dependency>
        	<groupId>net.htmlparser.jericho</groupId>
        	<artifactId>jericho-html</artifactId>
        	<version>3.2</version>
        </dependency>        
        <!-- test dependencies -->
        <dependency>
            <groupId>org.mule.tests</groupId>
            <artifactId>mule-tests-functional</artifactId>
            <version>${version}</version>
