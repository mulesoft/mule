    <flow name="echoHeisenberg">
        <heisenberg:execute-foreing-orders extensionName="Heisenberg" operationName="echoWithSignature"
                                           operationParameters="#[{ message: vars.message }]"/>
    </flow>

    <flow name="echoPetStore">
        <heisenberg:execute-foreing-orders extensionName="petstore" operationName="echoWithSignature"
                                           operationParameters="#[{ message: vars.message }]"/>
    </flow>

    <flow name="sayMyName">
        <heisenberg:execute-foreing-orders extensionName="Heisenberg" operationName="sayMyName"
                                           configName="#[vars.configName]"
                                           operationParameters="#[{}]"/>
    </flow>

    <flow name="echoStaticMessage">
        <heisenberg:execute-foreing-orders extensionName="Heisenberg" operationName="echoStaticMessage"
                                           operationParameters="#[{ message : vars.message}]"/>
    </flow>

    <flow name="echoWithoutMessage">
        <heisenberg:execute-foreing-orders extensionName="Heisenberg" operationName="echoWithSignature"
                                           operationParameters="#[{}]"/>
    </flow>


</mule>
