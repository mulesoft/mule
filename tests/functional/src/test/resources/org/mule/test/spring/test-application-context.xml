<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
    <bean id="org.mule.tck.testmodels.fruit.FruitBowl" class="org.mule.tck.testmodels.fruit.FruitBowl">
        <property name="fruit">
            <list>
                <ref local="org.mule.tck.testmodels.fruit.Apple"/>
                <ref local="org.mule.tck.testmodels.fruit.Banana"/>
            </list>
        </property>
    </bean>

    <bean id="java.lang.String" class="java.lang.String">
    </bean>

    <bean id="org.mule.tck.testmodels.fruit.Apple" class="org.mule.tck.testmodels.fruit.Apple">
    </bean>

    <bean id="org.mule.tck.testmodels.fruit.Banana" class="org.mule.tck.testmodels.fruit.Banana">
    </bean>

    <bean id="org.mule.tck.testmodels.fruit.Orange" class="org.mule.tck.testmodels.fruit.Orange">
    </bean>
</beans>