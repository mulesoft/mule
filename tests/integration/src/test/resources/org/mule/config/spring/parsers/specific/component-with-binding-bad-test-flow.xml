<?xml version="1.0" encoding="UTF-8"?>
<mule xmlns="http://www.mulesoft.org/schema/mule/core" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
               http://www.mulesoft.org/schema/mule/core http://www.mulesoft.org/schema/mule/core/current/mule.xsd">

    <flow name="service">
        <component-with-bindings class="org.mule.functional.functional.FunctionalTestComponent">
            <singleton-object class="org.mule.runtime.core.component.simple.StaticComponent"/>
            <binding interface="java.lang.String" method="setPrefix">
                <outbound-endpoint address="test://test"/>
            </binding>
        </component-with-bindings>
    </flow>
    
</mule>
