<?xml version="1.0"?>

<project>
    <extend>${basedir}/../project.xml</extend>
    <pomVersion>3</pomVersion>
    <artifactId>mule-multicast-provider</artifactId>
    <name>Mule Multicast Provider</name>
    <description>A Mule provider for Udp Multicasting. This provider supplies a Multicast Data Packet dispatcher and
        client implementation. for more information see the
        <a href="http://wiki.muleumo.org/display/MULE/Providers+Guide">Providers Guide</a>.
    </description>
    <shortDescription>multicast</shortDescription>
    <url>http://www.muleumo.org/doc/providers/multicast/</url>
    <dependencies>
        <dependency>
            <groupId>mule</groupId>
            <artifactId>mule-udp-provider</artifactId>
            <version>${pom.currentVersion}</version>
            <properties>
                <mule.core.jar>true</mule.core.jar>
            	<eclipse.dependency>true</eclipse.dependency>
            </properties>
        </dependency>
    </dependencies>
</project>
