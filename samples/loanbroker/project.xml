<project xmlns="http://maven.apache.org/POM/3.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://maven.apache.org/POM/3.0.0 http://maven.apache.org/maven-v3_0_0.xsd">
    <extend>${basedir}/../project.xml</extend>
    <name>Mule Loan Broker Sample</name>
    <id>mule-samples-loanbroker</id>
    <groupId>mule</groupId>
    <dependencies>
        <!-- Axis Deps -->
        <dependency>
            <artifactId>axis</artifactId>
            <groupId>axis</groupId>
            <version>${axis_version}</version>
            <url>http://ws.apache.org</url>
            <properties>
                <ignore>true</ignore>
            </properties>
        </dependency>
        <dependency>
            <groupId>geronimo-spec</groupId>
            <artifactId>geronimo-spec-saaj</artifactId>
            <version>${geronimo_spec_saaj_version}</version>
            <url>http://java.sun.com/products/saaj/</url>
            <properties>
                <ignore>true</ignore>
            </properties>
        </dependency>
        <dependency>
            <artifactId>axis-wsdl4j</artifactId>
            <groupId>axis</groupId>
            <version>${axis_wsdl4j_version}</version>
            <url>http://ws.apache.org</url>
            <properties>
                <ignore>true</ignore>
            </properties>
        </dependency>
        <!-- needed by the Axis Service component -->
        <dependency>
            <groupId>mule</groupId>
            <artifactId>mule-http-provider</artifactId>
            <version>${pom.currentVersion}</version>
            <url>http://muleumo.org/docs/modules/mule-http-provider/</url>
            <properties>
                <mule.core.jar>true</mule.core.jar>
                <eclipse.dependency>true</eclipse.dependency>
                <ignore>true</ignore>
            </properties>
        </dependency>
        <dependency>
            <groupId>mule</groupId>
            <artifactId>mule-tcp-provider</artifactId>
            <version>${pom.currentVersion}</version>
            <url>http://muleumo.org/docs/modules/mule-http-provider/</url>
            <properties>
                <mule.core.jar>true</mule.core.jar>
                <eclipse.dependency>true</eclipse.dependency>
                <ignore>true</ignore>
            </properties>
        </dependency>
        <dependency>
            <groupId>mule</groupId>
            <artifactId>mule-soap-provider</artifactId>
            <version>${pom.currentVersion}</version>
            <url>http://muleumo.org/docs/modules/mule-soap-provider/</url>
            <properties>
                <mule.core.jar>true</mule.core.jar>
                <eclipse.dependency>true</eclipse.dependency>
                <ignore>true</ignore>
            </properties>
        </dependency>
        <dependency>
            <id>commons-httpclient</id>
            <version>${commons_httpclient_version}</version>
            <url>http://jakarta.apache.org/commons/httpclient/index.html</url>
            <properties>
                <ignore>true</ignore>
            </properties>
        </dependency>
        <dependency>
            <id>commons-codec</id>
            <version>${commons_codec_version}</version>
            <url>http://jakarta.apache.org/commons/codec/index.html</url>
            <properties>
                <ignore>true</ignore>
            </properties>
        </dependency>
        <dependency>
            <groupId>javax.mail</groupId>
            <artifactId>mail</artifactId>
            <version>${javamail_version}</version>
            <url>http://java.sun.com/products/javamail/</url>
            <properties>
                <ignore>true</ignore>
            </properties>
        </dependency>
        
        <!-- XFire Deps (for testing only) -->
        <dependency>
            <groupId>stax</groupId>
            <artifactId>stax</artifactId>
            <version>${stax_version}</version>
            <url>http://jcp.org/en/jsr/detail?id=173</url>
            <properties>
                <ignore>true</ignore>
            </properties>
        </dependency>

        <dependency>
            <groupId>stax</groupId>
            <artifactId>stax-api</artifactId>
            <version>${stax_api_version}</version>
            <url>http://jcp.org/en/jsr/detail?id=173</url>
            <properties>
                <ignore>true</ignore>
            </properties>
        </dependency>

        <dependency>
            <groupId>wsdl4j</groupId>
            <artifactId>wsdl4j</artifactId>
            <version>${wsdl4j_version}</version>
            <url>http://www-124.ibm.com/developerworks/projects/wsdl4j/</url>
            <properties>
                <ignore>true</ignore>
            </properties>
        </dependency>

        <dependency>
            <groupId>org.codehaus.xfire</groupId>
            <artifactId>xfire-all</artifactId>
            <version>${xfire_all_version}</version>
            <url>http://xfire.codehaus.org</url>
            <properties>
                <ignore>true</ignore>
            </properties>
        </dependency>

        <dependency>
            <groupId>woodstox</groupId>
            <artifactId>wstx-asl</artifactId>
            <version>${wstx_asl_version}</version>
             <url>http://woodstox.codehaus.org</url>
            <properties>
                <ignore>true</ignore>
            </properties>
        </dependency>

        <dependency>
            <groupId>jdom</groupId>
            <artifactId>jdom</artifactId>
            <version>${jdom_version}</version>
            <properties>
                <ignore>true</ignore>
            </properties>
        </dependency>

        <dependency>
            <groupId>jaxen</groupId>
            <artifactId>jaxen</artifactId>
            <version>${jaxen_version}</version>
            <properties>
                <ignore>true</ignore>
            </properties>
        </dependency>

        <dependency>
            <groupId>geronimo-spec</groupId>
            <artifactId>geronimo-spec-saaj</artifactId>
            <version>${geronimo_spec_saaj_version}</version>
            <url>http://java.sun.com/products/saaj/</url>
            <properties>
                <ignore>true</ignore>
            </properties>
        </dependency>

       <!-- needed for WSDL client -->
        <dependency>
            <groupId>org.apache.ws.commons</groupId>
            <artifactId>XmlSchema</artifactId>
            <version>${XmlSchema_version}</version>
            <properties>
                <ignore>true</ignore>
            </properties>
        </dependency>

        <!-- Glue Deps (for testing only) -->
        <dependency>
            <artifactId>glue</artifactId>
            <groupId>glue</groupId>
            <version>${glue_version}</version>
            <properties>
                <ignore>true</ignore>
            </properties>
        </dependency>
 </dependencies>
</project>