<?xml version="1.0" encoding="UTF-8"?>
<mule xmlns="http://www.mulesource.org/schema/mule/core/2.0"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xmlns:spring="http://www.springframework.org/schema/beans"
      xmlns:vm="http://www.mulesource.org/schema/mule/vm/2.0"
      xsi:schemaLocation="
               http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
               http://www.mulesource.org/schema/mule/core/2.0 http://www.mulesource.org/schema/mule/core/2.0/mule.xsd
               http://www.mulesource.org/schema/mule/vm/2.0 http://www.mulesource.org/schema/mule/vm/2.0/mule-vm.xsd">

    <model name="main">
        <service name="WORMS">
            <rest-service-component serviceUrl="http://www.webservicex.net/stockquote.asmx/GetQuote"
                                    httpMethod="POST"
                                    urlFromMessage="true"
                                    errorExpression="ErrorExp">
                <not-filter>
                    <wildcard-filter pattern="*xyz*"/>
                </not-filter>
                <payloadParameterNames>
                    <spring:value>test-property1</spring:value>
                    <spring:value>test-property2</spring:value>
                </payloadParameterNames>
                <requiredParams key="r1" value="rv1"/>
                <requiredParams key="r2" value="rv2"/>
                <optionalParams key="o1" value="ov1"/>
                <optionalParams key="o2" value="ov2"/>
            </rest-service-component>
        </service>
    </model>
</mule>
