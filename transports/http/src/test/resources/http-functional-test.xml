<?xml version="1.0" encoding="UTF-8"?>
<mule xmlns="http://www.mulesource.org/schema/mule/core/2.0"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:spring="http://www.springframework.org/schema/beans"
       xmlns:http="http://www.mulesource.org/schema/mule/http/2.0"
        xmlns:test="http://www.mulesource.org/schema/mule/test/2.0"
    xsi:schemaLocation="
       http://www.mulesource.org/schema/mule/test/2.0 http://www.mulesource.org/schema/mule/test/2.0/mule-test.xsd
       http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
       http://www.mulesource.org/schema/mule/core/2.0 http://www.mulesource.org/schema/mule/core/2.0/mule.xsd
       http://www.mulesource.org/schema/mule/http/2.0 http://www.mulesource.org/schema/mule/http/2.0/mule-http.xsd">

    <http:endpoint name="clientEndpoint" address="http://localhost:60199" synchronous="true"/>

    <model name="main">
        <service name="testComponent">
            <inbound>
                <inbound-endpoint ref="clientEndpoint"/>
            </inbound>
            <test:component appendString=" Received"/>
        </service>
    </model>

</mule>
