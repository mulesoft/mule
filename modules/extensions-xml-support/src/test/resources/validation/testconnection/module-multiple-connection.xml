<?xml version="1.0" encoding="UTF-8"?>
<module name="module-multiple-connection"
        xmlns="http://www.mulesoft.org/schema/mule/module"
        xmlns:http="http://www.mulesoft.org/schema/mule/http"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="
           http://www.mulesoft.org/schema/mule/module http://www.mulesoft.org/schema/mule/module/current/mule-module.xsd
           http://www.mulesoft.org/schema/mule/http http://www.mulesoft.org/schema/mule/http/current/mule-http.xsd">

    <connection>
        <property name="someUserConfig" type="string" defaultValue="some-username"/>
        <property name="somePassConfig" type="string" defaultValue="some-password"/>
    </connection>

    <connection>
        <property name="anothaUserConfig" type="string" defaultValue="some-username"/>
        <property name="anothaPassConfig" type="string" defaultValue="some-password"/>
    </connection>
    <property name="port" type="string"/>
</module>
