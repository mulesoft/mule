              <xs:sequence>
                <xs:element minOccurs="0" maxOccurs="unbounded" name="number">
                  <xs:complexType>
                    <xs:attribute type="mule:expressionInt" use="required" name="value"></xs:attribute>
                  </xs:complexType>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
