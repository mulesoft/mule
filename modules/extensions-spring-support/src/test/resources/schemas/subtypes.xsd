        </xs:sequence>
        <xs:attribute type="mule:expressionString" use="required" name="myString"></xs:attribute>
        <xs:attribute type="mule:expressionList" use="optional" name="numbers"></xs:attribute>
      </xs:extension>
    </xs:complexContent>
