                <xs:complexContent>
                  <xs:extension xmlns:basic="http://www.mulesoft.org/schema/mule/basic" base="basic:org.mule.runtime.module.extension.internal.runtime.connectivity.basic.Owner"></xs:extension>
                </xs:complexContent>
              </xs:complexType>
            </xs:element>
