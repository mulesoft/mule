<?xml version='1.0' encoding='UTF-8'?>
<wsdl:definitions name="OnlineStoreService" targetNamespace="http://support.cxf.module.runtime.mule.org/"
    xmlns="http://support.cxf.module.runtime.mule.org/"
    xmlns:messages="http://www.nile-online.co.uk/schema/messages"
    xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">
    <wsdl:types>
        <xsd:schema targetNamespace="http://www.nile-online.co.uk/schema/messages"
            xmlns:messages="http://www.nile-online.co.uk/schema/messages"
            xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
            xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
            xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <xsd:element name="SearchItemRequest"/>
            <xsd:element name="SearchItemResponse"/>
        </xsd:schema>
    </wsdl:types>
    <wsdl:message name="SearchItemRequest">
        <wsdl:part element="messages:SearchItemRequest" name="SearchItemRequest"> </wsdl:part>
    </wsdl:message>
    <wsdl:message name="SearchItemResponse">
        <wsdl:part element="messages:SearchItemResponse" name="SearchItemResponse"> </wsdl:part>
    </wsdl:message>
    <wsdl:portType name="OnlineStorePort">
        <wsdl:operation name="SearchItem">
            <wsdl:input message="SearchItemRequest"> </wsdl:input>
            <wsdl:output message="SearchItemResponse"> </wsdl:output>
        </wsdl:operation>
    </wsdl:portType>
    <wsdl:binding name="OnlineStoreBinding" type="OnlineStorePort">
        <soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
        <wsdl:operation name="SearchItem">
            <soap:operation style="document"/>
            <wsdl:input>
                <soap:body use="literal"/>
            </wsdl:input>
            <wsdl:output>
                <soap:body use="literal"/>
            </wsdl:output>
        </wsdl:operation>
    </wsdl:binding>
    <wsdl:service name="OnlineStoreService">
        <wsdl:port binding="OnlineStoreBinding" name="OnlineStorePort">
            <soap:address location="http://localhost:8777/services/onlinestore"/>
        </wsdl:port>
    </wsdl:service>
</wsdl:definitions>
