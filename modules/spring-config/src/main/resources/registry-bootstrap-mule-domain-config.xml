<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
       xsi:schemaLocation="
       http://www.springframework.org/schema/util  http://www.springframework.org/schema/util/spring-util-current.xsd
       http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-current.xsd"
       default-init-method="initialise">


    <bean name="_muleSimpleRegistryBootstrap" class="org.mule.config.spring.SpringRegistryBootstrap">
        <property name="supportedArtifactType">
            <util:constant static-field="org.mule.config.bootstrap.ArtifactType.DOMAIN"/>
        </property>
    </bean>

</beans>
