<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" 
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-current.xsd">

    <bean id="apple" class="org.mule.tck.testmodels.fruit.Apple"/>
    
    <bean id="banana" class="org.mule.tck.testmodels.fruit.Banana"/>
    
    <bean id="orange" class="org.mule.tck.testmodels.fruit.Orange">
        <property name="brand" value="Tropicana"/>
    </bean>
    
    <bean id="kiwi" class="org.mule.tck.testmodels.fruit.Kiwi"/>
</beans>
