<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/3.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://maven.apache.org/POM/3.0.0 http://maven.apache.org/maven-v3_0_0.xsd">
    <extend>${basedir}/../../mule/project.xml</extend>
    <pomVersion>3</pomVersion>
    <id>mini-mule</id>
    <artifactId>mini-mule</artifactId>
    <name>Mini Mule</name>
    <shortDescription>mini-mule</shortDescription>
    <description>Is a light-weight mini distribution of Mule that includes the core plus any providers that do not have other dependencies, such as vm, tcp, udp, multicst, file, jms, stream, servlet, ssl and the mule client
    </description>
    <dependencies>
        <dependency>
            <groupId>mule</groupId>
            <artifactId>mule-core</artifactId>
            <version>${pom.currentVersion}</version>
            <type>jar</type>
             <properties>
                <mule.mini>true</mule.mini>
            </properties>
            <url>http://www.muleumo.org</url>
        </dependency>
        <dependency>
            <groupId>mule</groupId>
            <artifactId>mule-vm-provider</artifactId>
            <version>${pom.currentVersion}</version>
            <type>jar</type>
             <properties>
                <mule.mini>true</mule.mini>
            </properties>
            <url>http://muleumo.org/docs/modules/mule-vm-provider/</url>
        </dependency>
        <dependency>
            <groupId>mule</groupId>
            <artifactId>mule-file-provider</artifactId>
            <version>${pom.currentVersion}</version>
            <type>jar</type>
             <properties>
                <mule.mini>true</mule.mini>
            </properties>
            <url>http://muleumo.org/docs/modules/mule-file-provider/</url>
        </dependency>
        <dependency>
            <groupId>mule</groupId>
            <artifactId>mule-tcp-provider</artifactId>
            <version>${pom.currentVersion}</version>
            <type>jar</type>
             <properties>
                <mule.mini>true</mule.mini>
            </properties>
            <url>http://muleumo.org/docs/modules/mule-tcp-provider/</url>
        </dependency>
        <dependency>
            <groupId>mule</groupId>
            <artifactId>mule-ssl-provider</artifactId>
            <version>${pom.currentVersion}</version>
            <type>jar</type>
             <properties>
                <mule.mini>true</mule.mini>
            </properties>
            <url>http://muleumo.org/docs/modules/mule-ssl-provider/</url>
        </dependency>
        <dependency>
            <groupId>mule</groupId>
            <artifactId>mule-jms-provider</artifactId>
            <version>${pom.currentVersion}</version>
            <type>jar</type>
             <properties>
                <mule.mini>true</mule.mini>
            </properties>
            <url>http://muleumo.org/docs/modules/mule-jms-provider/</url>
        </dependency>
        <dependency>
            <groupId>mule</groupId>
            <artifactId>mule-udp-provider</artifactId>
            <version>${pom.currentVersion}</version>
            <type>jar</type>
             <properties>
                <mule.mini>true</mule.mini>
            </properties>
            <url>http://muleumo.org/docs/modules/mule-udp-provider/</url>
        </dependency>
        <dependency>
            <groupId>mule</groupId>
            <artifactId>mule-rmi-provider</artifactId>
            <version>${pom.currentVersion}</version>
            <type>jar</type>
             <properties>
                <mule.mini>true</mule.mini>
            </properties>
            <url>http://muleumo.org/docs/modules/mule-rmi-provider/</url>
        </dependency>
        <dependency>
            <groupId>mule</groupId>
            <artifactId>mule-ejb-provider</artifactId>
            <version>${pom.currentVersion}</version>
            <type>jar</type>
             <properties>
                <mule.mini>true</mule.mini>
            </properties>
            <url>http://www.muleumo.org</url>
        </dependency>
        <dependency>
            <groupId>mule</groupId>
            <artifactId>mule-multicast-provider</artifactId>
            <version>${pom.currentVersion}</version>
            <type>jar</type>
             <properties>
                <mule.mini>true</mule.mini>
            </properties>
            <url>http://muleumo.org/docs/modules/mule-ejb-provider/</url>
        </dependency>
        <dependency>
            <groupId>mule</groupId>
            <artifactId>mule-stream-provider</artifactId>
            <version>${pom.currentVersion}</version>
            <type>jar</type>
             <properties>
                <mule.mini>true</mule.mini>
            </properties>
            <url>http://www.muleumo.org</url>
        </dependency>
        <dependency>
            <groupId>mule</groupId>
            <artifactId>mule-extras-client</artifactId>
            <version>${pom.currentVersion}</version>
            <type>jar</type>
             <properties>
                <mule.mini>true</mule.mini>
            </properties>
            <url>http://muleumo.org/docs/modules/mule-extras-client/</url>
        </dependency>
        <dependency>
            <groupId>mule</groupId>
            <artifactId>mule-http-provider</artifactId>
            <version>${pom.currentVersion}</version>
            <type>jar</type>
             <properties>
                <mule.mini>true</mule.mini>
            </properties>
            <url>http://muleumo.org/docs/modules/mule-http-provider/</url>
        </dependency>
        <dependency>
            <artifactId>commons-codec</artifactId>
            <groupId>commons-codec</groupId>
            <version>${commons_codec_version}</version>
            <properties>
                <mule.mini>true</mule.mini>
            </properties>
        </dependency>
        <dependency>
            <artifactId>commons-httpclient</artifactId>
            <groupId>commons-httpclient</groupId>
            <version>${commons_httpclient_version}</version>
            <url>http://jakarta.apache.org/commons/httpclient/index.html</url>
            <properties>
                <mule.mini>true</mule.mini>
            </properties>
        </dependency>
    </dependencies>
</project>
